{
  "name": "Raindrop",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#f5f5f5",
    "editor.foreground": "#333333",
    "sideBar.background": "#dedddd",
    "sideBar.foreground": "#333333",
    "sideBarSectionHeader.foreground": "#333333",
    "sideBarSectionHeader.background": "#dedddd",
    "list.activeSelectionForeground": "#333333",
    "list.activeSelectionBackground": "#eeeeee",
    "list.hoverBackground": "#e7e7e7",
    "list.highlightForeground": "#333333",
    "activityBar.background": "#dedddd",
    "activityBar.foreground": "#333333",
    "titleBar.activeBackground": "#c4b7d7",
    "statusBar.background": "#c4b7d7",
    "statusBar.foreground": "#333333",
    "statusBar.noFolderBackground": "#c4b7d7",
    "statusBar.debuggingBackground": "#c4b7d7",
    "statusBarItem.remoteBackground": "#c4b7d7",
    "list.errorForeground": "#cc5555",
    "gitDecoration.modifiedResourceForeground": "#333333",
    "gitDecoration.untrackedResourceForeground": "#333333"
  },
  "tokenColors": [
    
    {
      "scope": "comment",
      "settings": {
        "foreground": "#c4b7d7",
      },
    },
    
    {
      "scope": "keyword, storage, support.type.stdint, punctuation.separator.inheritance.ruby",
      "settings": {
        "foreground": "#5f7acc",
      },
    },
    
    {
      "scope": "entity.other.inherited-class, entity.name.tag, entity.name.class",
      "settings": {
        "foreground": "#7f4f9b",
      },
    },
    
    {
      "scope": "string",
      "settings": {
        "foreground": "#65a34c",
      },
    },
    
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#bca46d",
      },
    },
    
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#cc5555",
      },
    },
    
  ]
}
