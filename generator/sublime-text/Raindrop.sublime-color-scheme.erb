{
  "name": "Raindrop Color Scheme",
  "globals":
  {
    "background": "<%= background %>",
    "foreground": "<%= foreground %>",
    "caret": "<%= cursor %>",
    "selection": "<%= selection %>",
  },
  "rules":
  [
    <% text_mate_scopes.each do |s| %>
    {
      "scope": "<%= s['scope'] %>",
      "foreground": "<%= s['foreground'] %>",
    },
    <% end %>
  ],
}
