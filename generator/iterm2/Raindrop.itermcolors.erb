<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <% ansi_colors.each_with_index do |c, i| %>
  <%= partial 'itermcolor', { name: "Ansi #{i} Color", color: c } %>
  <% end %>
  <%= partial 'itermcolor', { name: 'Background Color', color: background } %>
  <%= partial 'itermcolor', { name: 'Cursor Color', color: cursor } %>
  <%= partial 'itermcolor', { name: 'Cursor Text Color', color: white } %>
  <%= partial 'itermcolor', { name: 'Foreground Color', color: foreground } %>
  <%= partial 'itermcolor', { name: 'Link Color', color: purple } %>
  <%= partial 'itermcolor', { name: 'Selected Text Color', color: foreground } %>
  <%= partial 'itermcolor', { name: 'Selection Color', color: selection } %>
</dict>
</plist>
