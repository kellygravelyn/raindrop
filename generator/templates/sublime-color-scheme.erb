{
  "name": "Raindrop Color Scheme",
  "globals":
  {
    "background": "<%= BACKGROUND %>",
    "foreground": "<%= FOREGROUND %>",
    "caret": "<%= CURSOR %>",
    "selection": "<%= SELECTION %>",
  },
  "rules":
  [
    {
      "scope": "comment",
      "foreground": "<%= PURPLE %>",
    },
    {
      "scope": "keyword",
      "foreground": "<%= BLUE %>",
    },
    {
      "scope": "storage",
      "foreground": "<%= BLUE %>",
    },
    {
      "scope": "entity.name",
      "foreground": "<%= DARK_PURPLE %>",
    },
    {
      "scope": "entity.other.inherited-class",
      "foreground": "<%= DARK_PURPLE %>",
    },
    {
      "scope": "string",
      "foreground": "<%= GREEN %>",
    },
    {
      "scope": "constant.numeric",
      "foreground": "<%= YELLOW %>",
    },
    {
      "scope": "invalid",
      "foreground": "<%= RED %>"
    },
  ],
}
